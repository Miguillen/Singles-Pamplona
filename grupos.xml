<ScrollView  xmlns:android = "http://schemas.android.com/apk/res/android" 
    android:id = "@+id/ScrollView01" 
    android:layout_width = "fill_parent" 
    android:layout_height = "fill_parent" >

	<RelativeLayout 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/fondo"
    android:orientation="vertical"
    android:paddingBottom="10dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:scrollbars="vertical" >
 
    <TextView
        android:id="@+id/treinta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:background="@drawable/textview"
        android:clickable="true"
        android:drawableLeft="@drawable/treinta"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="@dimen/ochodp"
        android:onClick="treintaytantos"
        android:padding="8dp"
        android:text="@string/treinta"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/cuarenta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/treinta"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/cuarenta"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="cuarentaanneros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/cuarenta"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
    
    <TextView
        android:id="@+id/cuarentones"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cuarenta"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/cuarentones"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="cuarentones"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/cuarentones"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/nuevos"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cuarentones"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/nuevos"
        android:ellipsize="marquee"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="nuevos"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/nuevos"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/amor"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/nuevos"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/corazones"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="sebuscaamor"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/amor"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	 <TextView
        android:id="@+id/veinte"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/amor"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/veinte"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="veintitantos"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/veinte"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/viajeros"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/veinte"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/viajeros"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="viajeros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/viajeros"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/tudela"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/viajeros"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/tudelanos"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="singlestudela"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/tudela"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/cincuenta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tudela"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/cincuenta"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="cincuentanneros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/cincuenta"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/rochapeanos"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cincuenta"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/rochapeos"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="rochapeanos"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/rochapeanos"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/montanneros"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rochapeanos"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/caminatas"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:ellipsize="marquee"
        android:gravity="left|center"
        android:onClick="montanneros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/montanneros"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/lastminute"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/montanneros"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/lastminute"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="lastminute"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/lastminute"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/fiestadelclan"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/lastminute"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/fiestaclan"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="fiestadelclan"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/fiestadelclan"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	
	    <TextView
        android:id="@+id/actividadesculturales"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/fiestadelclan"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/culturales"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="actividadesculturales"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/actividadesculturales"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/actividadesalairelibre"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/actividadesculturales"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/airelibre"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="actividadesalairelibre"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/actividadesalairelibre"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/padres"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/actividadesalairelibre"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/padres"
        android:drawablePadding="10dp"
        android:ellipsize="marquee"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="papasymamassingleconninnos"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/padres"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/literatura"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/padres"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/literatura"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="literaturaarteylibros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/literatura"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/rockeros"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/literatura"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/rockeros"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="rockeros"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/rockeros"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/cocina"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rockeros"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/cocina"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="grupodecocina"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/cocina"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />
	<TextView
        android:id="@+id/conciertos"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/cocina"
        android:background="@drawable/textview"
        android:drawableLeft="@drawable/conciertos"
        android:drawablePadding="10dp"
        android:clickable="true"
        android:gravity="left|center"
        android:onClick="conciertos"
        android:padding="8dp"
        android:layout_marginBottom = "2dp"
        android:text="@string/conciertos"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold" />

	</RelativeLayout>
</ScrollView>